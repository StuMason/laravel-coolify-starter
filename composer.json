{
    "name": "stumason/laravel-coolify-starter",
    "description": "Transform a fresh Laravel app into a production-ready Coolify deployment with Horizon, Reverb, Telescope, and health checks.",
    "keywords": ["laravel", "coolify", "deployment", "horizon", "reverb", "telescope", "nixpacks"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stu Mason",
            "email": "stu@stumason.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Stumason\\CoolifyStarter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stumason\\CoolifyStarter\\CoolifyStarterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
